{
  "name": "vscode-angular-generator",
  "displayName": "Angular File Generator for VSCode",
  "description": "Angular File Generator for VSCode editor",
  "version": "0.1.0",
  "preview": true,
  "icon": "icon.png",
  "license": "MIT",
  "publisher": "imgildev",
  "author": {
    "name": "Manuel Gil",
    "url": "https://github.com/ManuelGil"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/manuelgil"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "angular-cli-file-generator",
    "angular-cli-file",
    "angular-cli-generator",
    "angular-cli",
    "angular-file-generator",
    "angular-file",
    "angular-generator",
    "angular",
    "angular2",
    "angularjs",
    "commands",
    "extension",
    "file",
    "generator",
    "javascript",
    "js",
    "make",
    "ng",
    "snippet",
    "template",
    "terminal",
    "ts",
    "typescript",
    "vscode-angular-cli-file-generator",
    "vscode-angular-cli-file",
    "vscode-angular-cli",
    "vscode-angular-file-generator",
    "vscode-angular-file",
    "vscode-angular-generator",
    "vscode-angular",
    "vscode-angular",
    "vscode-extension",
    "vscode"
  ],
  "homepage": "https://github.com/ManuelGil/vscode-angular-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuelGil/vscode-angular-generator"
  },
  "bugs": {
    "url": "https://github.com/ManuelGil/vscode-angular-generator/issues"
  },
  "activationEvents": [
    "onCommand:angular.file.class",
    "onCommand:angular.terminal.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "angular.file.class",
        "title": "Angular: Make Class"
      },
      {
        "command": "angular.terminal.start",
        "title": "Angular: Start Server"
      }
    ],
    "submenus": [
      {
        "id": "angular.submenu",
        "label": "AngularJS File Generator"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "angular.submenu",
          "group": "1_modification",
          "when": "explorerViewletFocus && explorerResourceIsFolder"
        }
      ],
      "angular.submenu": [
        {
          "command": "angular.file.class"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "glob": "^7.1.6",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "prettier": "3.0.3",
    "typescript": "^4.9.5",
    "vscode-test": "^1.5.0"
  }
}
